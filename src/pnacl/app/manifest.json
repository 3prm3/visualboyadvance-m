{
  "name": "VBA-M",
  "description": "A native GBA emulator for Chrome",
  "version": "1.3.6",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "offline_enabled": true,
  "icons": {
    "128": "icon.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "syncFileSystem",
    "fileSystem",
    "storage",
    "unlimitedStorage"
  ]
}
